<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="@color/white" />
			<stroke android:width="0.0dip" android:color="@color/white" />
			<corners android:radius="6.0dip" />
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/load_bg" />
			<stroke android:width="0.0dip" android:color="@color/load_bg" />
			<corners android:radius="6.0dip" />
		</shape>
	</item>
</selector>